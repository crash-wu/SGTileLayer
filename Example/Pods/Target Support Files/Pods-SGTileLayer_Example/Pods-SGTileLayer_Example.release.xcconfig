CLANG_ENABLE_MODULES = YES
ENABLE_BITCODE = NO
FRAMEWORK_SEARCH_PATHS = $(inherited) $(HOME)/Library/SDKs/ArcGIS/iOS "$PODS_CONFIGURATION_BUILD_DIR/SGTileLayer"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_CONFIGURATION_BUILD_DIR/SGTileLayer/SGTileLayer.framework/Headers"
OTHER_LDFLAGS = $(inherited) -ObjC -l"c++" -framework "ArcGIS" -framework "SGTileLayer"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
